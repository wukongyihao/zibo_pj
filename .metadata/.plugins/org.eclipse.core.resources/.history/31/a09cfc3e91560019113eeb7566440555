package prac;

public class testctrl {

	public static void ctrl() {
		for(int i = 1;i<=9;i++) {
			for(int j = 1;j<=i;j++) {
				System.out.print(i +"*" +j +"="+ i*j);
			}
			System.out.println();
		}
		
		
	}
	public static void main(String[] args) {
		 testctrl ctpr = new testctrl();
		 ctpr.ctrl();
		 
	}
	
	
	
	
}
