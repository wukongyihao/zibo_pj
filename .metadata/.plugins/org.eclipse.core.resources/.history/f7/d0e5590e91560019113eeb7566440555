package prac;

public class testctrl {

	public static void ctrl() {
		int i = 1;
		int j = 1;
		for(;i<=9;i++) {
			for(;j==i;j++) {
				System.out.println(i +"*" +j +"="+ i*j);
			}
		}
		
		
	}
	public static void main(String[] args) {
		 testctrl ctpr = new testctrl();
		 ctpr.ctrl();
		 
	}
	
	
	
	
}
